@{
    ViewBag.Title = "Sitemap";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
    
   
}
@model ProductsEStore.Models.SiteMapData
<h2>
    Sitemap</h2>
<div class="container">
    <div class="row">
        <div class="col-lg-3">
            <div class="row">
                <div class="col-lg-12">
                    Pages:
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    Book Categories:
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    @Html.Partial("_MonthlyPartial", Model.MonthlyData)
                </div>
            </div>
        </div>
        <div class="col-lg-9">
            <div class="row">
                <div class="col-lg-12">
                    @Html.Partial("_PopularTagsPartial", Model.PopularTagData)
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                </div>
            </div>
        </div>
    </div>
</div>
